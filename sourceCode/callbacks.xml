<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<?xml-stylesheet type="text/xsl" href="callbacks.xsl"?>

<plugin name="simLuaParser" author="federico.ferri.it@gmail.com">
    <description>API functions for LuaParser plugin.</description>
    <command name="parse">
        <description>Parse lua code</description>
        <params>
            <param name="code" type="string">
                <description>lua code</description>
            </param>
        </params>
        <return>
            <param name="result" type="string">
                <description>dump of syntax tree</description>
            </param>
        </return>
    </command>
    <command name="getFunctionDefs">
        <description>Parse lua code</description>
        <params>
            <param name="code" type="string">
                <description>lua code</description>
            </param>
        </params>
        <return>
            <param name="result" type="string">
                <description>dump of syntax tree</description>
            </param>
        </return>
    </command>
</plugin>
